@model Agro.Model.Dto.Request.RequestDto

<div>
    <h4>Запрос поставщику</h4>
    <hr />
    <dl class="row">        
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.City)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.City.Name)
        </dd>
        Контакты
        @foreach (var intention in Model.Intentions)
        {
            <div>контактный номер @intention.User.PhoneNumber</div><br />
        }

    </dl>
</div>
<div>    
    <a asp-action="Index">К списку запросов</a>
</div>

